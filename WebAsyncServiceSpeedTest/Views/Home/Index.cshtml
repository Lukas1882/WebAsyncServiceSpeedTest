@{
    ViewData["Title"] = "Home Page";
}
<br />
<div id="myCarousel" class="carousel slide" data-ride="carousel" data-interval="6000">
    <div class="row">
        <div class="col-lg-12">
            <button id="apiBtn1" onclick="callAPI1()" class="btn btn-info">Call API 1</button>
        </div>
    </div>
    <br />
    <div class="row">
        <div class="col-lg-12">
            <button id="apiBtn2" onclick="callAPI2()" class="btn btn-success">Call API 2</button>
        </div>
    </div>
    <br />
    <div class="row">
        <div class="col-lg-12">
            <button id="apiBtn3" onclick="callAPI3()" class="btn btn-danger">Call API 1 and 2</button>
        </div>
    </div>
    <br />
    <div class="row">
        <div class="col-lg-12">
            <a id="time">time</a>

        </div>
    </div>
</div>

<script>
    function callAPI1() {
        $.ajax({
            type: "GET",
            url: "~/api/server/GetAPI1",
            contentType: "application/json; charset=utf-8",
            dataType: "json",
            success: function (response) {
                printTime();
            },
            failure: function (response) {
                alert(response.responseText);
            },
            error: function (response) {
                alert(response.responseText);
            }
        });

    }
    function printTime() {
        alert(1);
    }

</script>

